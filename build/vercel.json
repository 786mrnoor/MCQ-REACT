{
    "builds": [
        {
            "src": "/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/asset-manifest.json",
            "dest": "/asset-manifest.json"
        },
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/logo192.png",
            "dest": "/static/logo192.png"
        },
        {
            "src": "/logo512.png",
            "dest": "/static/logo512.png"
        },
        {
            "src": "/logo512.png",
            "dest": "/static/logo512.png"
        },
        {
            "src": "/manifest.json",
            "dest": "/manifest.json"
        },
        {
            "src": "/robots.txt",
            "dest": "/robots.txt"
        },
        {
            "src": "/service-worker.js",
            "dest": "/service-worker.js"
        },
        {
            "src": "/service-worker.js.map",
            "dest": "/service-worker.js.map"
        },

        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}